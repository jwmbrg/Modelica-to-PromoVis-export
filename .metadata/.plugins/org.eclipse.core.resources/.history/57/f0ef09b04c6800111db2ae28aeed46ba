class pmv_variable(object):
    '''
    classdocs
    '''
    class _type_params(object):
        _input=0
        _state=1
        _disturbance=2
        
    def __init__(self,E,A,B,F,g,state_names, input_names):
        '''
        Constructor
        '''
        self.E=E
        self.A=A
        self.B=B
        self.F=F
        self.g=g
        self.state_names=state_names
        self.input_names=input_names
        self.createModel();
        
        
    def createModel(self):
        return
    
    
    def createVar(self,index,type):
        name=0
        type=0
        state_dict=0
    
        input_dict=0
        disturbance_dict=0
        
        if(type==self._type_params._input):
            name=self.state_names.get(index)
        elif(type==self._type_params._state):
            name=self.state_names.get(index)
        elif(type==self._type_params._disturbance):
            name=self.state_names.get(index)
        
        return pmv_variable(name,type,state_dict,input_dict,disturbance_dict)
        